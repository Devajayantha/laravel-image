{
  "name": "ankitpokhrel/laravel-image",
  "description": "Easy image upload and thumbnail management",
  "version": "dev-master",
  "license": "MIT",
  "authors": [
    {
      "name": "Ankit Pokhrel",
      "email": "ankitpokhrel@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "~5.0",
    "league/glide-laravel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.0",
    "mockery/mockery": "dev-master",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "AnkitPokhrel\\LaravelImage\\": "src/LaravelImage"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnkitPokhrel\\LaravelImage\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
