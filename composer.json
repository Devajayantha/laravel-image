{
  "name": "ankitpokhrel/laravel-image",
  "description": "Easy image upload and thumbnail management",
  "version": "dev-master",
  "license": "MIT",
  "authors": [
    {
      "name": "Ankit Pokhrel",
      "email": "ankitpokhrel@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.1.*",
    "league/glide-laravel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "mockery/mockery": "dev-master",
    "squizlabs/php_codesniffer": "2.*",
    "orchestra/testbench": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "AnkitPokhrel\\LaravelImage\\": "src/LaravelImage"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnkitPokhrel\\LaravelImage\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
